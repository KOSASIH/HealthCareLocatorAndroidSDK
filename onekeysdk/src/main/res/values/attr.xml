<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OneKeyTextView">
        <attr name="setFont" format="string" />
        <attr name="boldText" format="boolean" />
        <attr name="forceTextSize" format="boolean" />
        <attr name="title1Style" format="boolean" />
        <attr name="title2Style" format="boolean" />
        <attr name="title3Style" format="boolean" />
        <attr name="searchInputStyle" format="boolean" />
        <attr name="smallStyle" format="boolean" />
        <attr name="OneKey_buttonStyle" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ExpandableLayout">
        <attr name="el_duration" format="integer" />
        <attr name="el_expanded" format="boolean" />
        <attr name="el_parallax" format="float" />
        <attr name="android:orientation" />
    </declare-styleable>
</resources>